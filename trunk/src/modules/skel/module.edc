	         	program 
	            {
		            name: "navbar_show";
		            source: "bar";
		            signal: "show";		            
		            action: STATE_SET "default" 0.0;
		            target: "bar";
		            target: "home";
		            target: "good";
		            target: "bad";
		            target: "textbar";	
		            target: "next";            
		            target: "previous";		                        
		        } 	            
	         	program 
	            {
		            name: "navbar_hide";
		            source: "bar";
		            signal: "hide";		            
		            action: STATE_SET "hide" 0.0;
		            target: "bar";
		            target: "home";
		            target: "good";
		            target: "bad";
		            target: "textbar";	
		            target: "next";            
