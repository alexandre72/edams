images
{
	image: "navbar.png" COMP;
	image: "good.png" COMP;
    image: "home.png" COMP;
	image: "bad.png" COMP;
	image: "previous.png" COMP;
	image: "next.png" COMP;		
}



collections 
{
    group
     {
        name: "interface";
        min: 640 480;
        parts
        {
            part
            {
                name: "background";
                //type: RECT;
                type: SWALLOW;
                mouse_events: 1;                
    	        description
	    	    {
                    state: "default" 0.0;
      		        color: 0 0 50 255;
                }
            }     
                            
                     
			part
            {
                name: "bar";
                type: IMAGE;
                mouse_events: 1;
                description
                {
                    state: "default" 0.0;
                    visible:1;
                    rel1.to: "background";
                    rel2.to: "background";                    
                    rel1.relative: 0 0.93;
                    rel2.relative: 0.45 1.0;
                    image.normal: "navbar.png";
                }     
                description
                {      
		            state: "hide" 0.0;                           
                    visible:0;
                }
            }

			part
            {
                name: "textbar";
                type: TEXT;
                mouse_events: 1;
                effect: PLAIN ;
                description
                {
                    state: "default" 0.0;
                    visible:1;
                    text.align: 0 0.5;                    
		            color: 0 0 0 255;
                    text.font: "Sans";
                    text.size: 15;  
                    rel1.to: "bar";
                    rel2.to: "bar";                    
                    rel1.relative: 0.15 0.5;
                    rel2.relative: 0.55 0.6;                                      
                }     
                description
                {      
		            state: "hide" 0.0;                           
                    visible:0;
                }
            }
            
         	part
            {
                name: "previous";
                type: IMAGE;
                description
                {
                    state: "default" 0.0;                
					aspect: 1 1;
					aspect_preference: BOTH;
                    rel1.to: "bar";
                    rel2.to: "bar";                    
                    rel1.relative: 0.58 0.2;
                    rel2.relative: 0.64 0.8;
                    image.normal: "previous.png";
                }
                
		        description 
		        {
		            state: "bigger" 0.0;
		            inherit: "default" 0.0;
		            rel1.offset: -1 -1;
		            rel2.offset: 1 1;
		        } 
		        
		        description 
		        {
		            state: "smaller" 0.0;
		            inherit: "default" 0.0;
		            rel1.offset: 1 1;
		            rel2.offset: -1 -1;
		        }
                description
                {      
		            state: "hide" 0.0;  
                    visible:0;
                }		                        
            }             
            
            
