		            name: "good_anim_start";
		            source: "good_anim";
		            signal: "show";            
		            action: STATE_SET "show" 0.0;
		            transition: SINUSOIDAL 0.5;
		            target: "good_anim"; 
		            after: "good_anim_end";
	            }
	         	program 
	            {
		            name: "good_anim_end";
