	            {
		            name: "home_mouse_in";
                    signal: "mouse,in";
		            source: "home";
		            action: STATE_SET "bigger" 0.0;
        		    transition: LINEAR 0.1;
		            target: "home";
                }
	            program 
	            {
		            name: "home_mouse_out";
		            signal: "mouse,out";
		            source: "home";
		            action: STATE_SET "default" 0.0;
		            target: "home";
	            }  
	         	program 
	            {
		            name: "home_mouse_click";
		            signal: "mouse,clicked,*";
		            source: "home";
		            action: STATE_SET "smaller" 0.0;
		            transition: LINEAR 0.1;
		            target: "home";
	            } 		            
	                  
	            program 
	            {
		            name: "bad_mouse_in";
